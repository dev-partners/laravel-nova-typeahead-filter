{
    "name": "devpartners/typeahead-filter",
    "description": "A Laravel Nova select filter with typeahead.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Devpartners\\TypeaheadFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devpartners\\TypeaheadFilter\\FilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
